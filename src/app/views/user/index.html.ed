<table border="1">
	<% foreach(User user ; controller._users) { %>
	<tr>
		<td>
			<ul>
				<li><%=user.name%></li>
				<li><%=user.email%></li>
			</ul>
		</td>
	</tr>
	<% } %>
	<% if(controller._users.length == 0) { %>
	<tr>
		<td>There are no users.</td>
	</tr>
	<% } %>
</table>

<%= controller._things %>

