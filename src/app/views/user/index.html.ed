<table border="1">
	<% foreach(User user ; controller._users) { %>
	<tr>
		<td>
			<ul>
				<li><%=user.name%></li>
				<li><%=user.user_name%></li>
				<li><%=user.disabled ? "True" : "False" %></li>
				<li><%=user.email%></li>
			</ul>
		</td>
	</tr>
	<% } %>
</table>

<%= controller._things %>

